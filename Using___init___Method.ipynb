        "class TV:\n",
        "    def __init__(self, brand, size):\n",
        "        self.brand = brand\n",
        "        self.size = size\n",
        "\n",
        "    def display(self):\n",
        "        print(f\"TV Brand: {self.brand}, Size: {self.size} inches, Created by Vinayak\")\n",
        "\n",
        "tv = TV(\"Samsung\", 55)\n",
        "tv.display()\n"
      ],
