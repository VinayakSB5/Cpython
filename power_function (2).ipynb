      "source": [
        "def power(base, exp):\n",
        "    if exp == 0:\n",
        "        return 1\n",
        "    return base * power(base, exp - 1)\n",
        "\n",
        "print(\"vinayaksb\")\n",
        "base = int(input(\"Enter base: \"))\n",
        "exp = int(input(\"Enter exponent: \"))\n",
        "print(\"Result:\", power(base, exp))\n",
        "\n"
