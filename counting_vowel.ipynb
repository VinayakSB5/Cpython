        "def count_vowels(s):\n",
        "    return sum(1 for char in s.lower() if char in 'aeiou')\n",
        "\n",
        "print(\"vinayaksb\")\n",
        "s = input(\"Enter a string: \")\n",
        "print(\"Vowel Count:\", count_vowels(s))\n"
